{
  "base": "semantic\\",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src\\semantic\\",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "src\\semantic\\dist\\",
      "uncompressed": "src\\semantic\\dist\\components\\uncompressed\\",
      "compressed": "src\\semantic\\dist\\components\\",
      "themes": "dist/themes/"
    },
    "clean": "dist/"
  },
  "permission": false,
  "autoInstall": false,
  "rtl": false,
