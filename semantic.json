    "label",
    "list",
    "loader",
    "segment",
    "step",
    "breadcrumb",
    "form",
    "grid",
    "menu",
    "message",
    "table",
    "card",
    "comment",
    "feed",
    "item",
    "statistic",
    "accordion",
    "checkbox",
    "dropdown",
    "modal",
    "popup",
    "progress",
    "rating",
    "search",
    "sidebar",
    "tab",
    "state"
  ]
